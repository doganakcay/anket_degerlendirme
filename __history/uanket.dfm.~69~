  object qdegerlendirme: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO DA_ANKET_DEGERLERDIRME'
      '  (ANKET_DEG_ID, ANKET_SORU_ID, ANKET_ID, CEVAP)'
      'VALUES'
      '  (:ANKET_DEG_ID, :ANKET_SORU_ID, :ANKET_ID, :CEVAP)')
    SQLDelete.Strings = (
      'DELETE FROM DA_ANKET_DEGERLERDIRME'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLUpdate.Strings = (
      'UPDATE DA_ANKET_DEGERLERDIRME'
      'SET'
      
        '  ANKET_DEG_ID = :ANKET_DEG_ID, ANKET_SORU_ID = :ANKET_SORU_ID, ' +
        'ANKET_ID = :ANKET_ID, CEVAP = :CEVAP'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLLock.Strings = (
      
        'SELECT ANKET_DEG_ID, ANKET_SORU_ID, ANKET_ID, CEVAP FROM DA_ANKE' +
        'T_DEGERLERDIRME'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT ANKET_DEG_ID, ANKET_SORU_ID, ANKET_ID, CEVAP FROM DA_ANKE' +
        'T_DEGERLERDIRME'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLRecCount.Strings = (
      'SELECT Count(*) FROM ('
      
        'SELECT DA_ANKET_DEGERLERDIRME.ROWID, DA_ANKET_DEGERLERDIRME.* FR' +
        'OM DA_ANKET_DEGERLERDIRME'
      ''
      ')')
    Session = OraSession1
    SQL.Strings = (
      'select t.*, S.SORU, t.rowid'
      '  from DA_ANKET_DEGERLERDIRME t, DA_ANKET_SORULAR S'
      ' WHERE S.ANKET_SORU_ID = T.ANKET_SORU_ID'
      '   AND S.ANKET_ID =:ank  AND T.ANKET_DEG_ID=:DENk'
      ' ORDER BY S.ANKET_ID, T.ANKET_DEG_ID, T.ANKET_SORU_ID')
    AfterPost = qdegerlendirmeAfterPost
    Left = 960
    Top = 120
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ank'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'DENk'
        Value = Null
      end>
