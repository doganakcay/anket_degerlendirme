    SQLRefresh.Strings = (
      'SELECT ANKET_ID, ANKET_BASLIK, GRUP, AKTIF FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLRecCount.Strings = (
      'SELECT Count(*) FROM ('
      'SELECT DA_ANKET.ROWID, DA_ANKET.* FROM DA_ANKET'
      ''
      ')')
    Session = OraSession1
    SQL.Strings = (
      'select  t.UZ_TARIH,'
      '       t.anket_id,'
      '       t.ANKET_BASLIK,'
      '       t.ANKET_SORU_ID,'
      '       t.SORU,'
      '       sum(t.T_Kat'#305'l'#305'yorum) T_Kat'#305'l'#305'yorum,'
      '       sum(t.Kat'#305'l'#305'yorum) Kat'#305'l'#305'yorum,'
      '       sum(t.Karars'#305'z'#305'm) Karars'#305'z'#305'm,'
      '       sum(t.Kat'#305'lm'#305'yorum) Kat'#305'lm'#305'yorum,'
      '       sum(t.K_Kat'#305'lm'#305'yorum) K_Kat'#305'lm'#305'yorum,'
      '       sum(t.toplam) TOPLAM,'
      '       t.ASGARI_ANKET,'
      
        '      TRUNC( (sum(t.T_Kat'#305'l'#305'yorum)/Sum(t.toplam))*100,2) T_Kat'#305'l' +
        #305'yorum_YUZDE,'
      
        '      TRUNC( (sum(t.Kat'#305'l'#305'yorum)/Sum(t.toplam))*100,2) Kat'#305'l'#305'yor' +
        'um_YUZDE,'
      
        '      TRUNC( (sum(t.Karars'#305'z'#305'm)/Sum(t.toplam))*100,2) Karars'#305'z'#305'm' +
        '_YUZDE,'
      
        '      TRUNC( (sum(t.Kat'#305'lm'#305'yorum)/Sum(t.toplam))*100,2) Kat'#305'lm'#305'y' +
        'orum_YUZDE,'
      
        '      TRUNC( (sum(t.K_Kat'#305'lm'#305'yorum)/Sum(t.toplam))*100,2) K_Kat'#305 +
        'lm'#305'yorum_YUZDE     '
      '       '
      '  from DA_ANKET_DEG_VIEW t'
      ' where 1 = 1'
      '/*and t.anket_id=6 --*/ %s'
      ''
      ' group by t.UZ_TARIH,'
      '          t.anket_id,'
      '          t.ANKET_BASLIK,'
      '          t.ANKET_SORU_ID,'
      '          t.SORU,'
      '          t.ASGARI_ANKET'
      '  ORDER BY T.anket_id,T.ANKET_SORU_ID')
    MasterSource = DSANKET
    MasterFields = 'ANKET_ID'
    Left = 496
    Top = 352
  end
