  object QGENEL_BILGI: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO DA_ANKET'
      '  (ANKET_ID, ANKET_BASLIK, GRUP, AKTIF)'
      'VALUES'
      '  (:ANKET_ID, :ANKET_BASLIK, :GRUP, :AKTIF)')
    SQLDelete.Strings = (
      'DELETE FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLUpdate.Strings = (
      'UPDATE DA_ANKET'
      'SET'
      
        '  ANKET_ID = :ANKET_ID, ANKET_BASLIK = :ANKET_BASLIK, GRUP = :GR' +
        'UP, AKTIF = :AKTIF'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLLock.Strings = (
      'SELECT ANKET_ID, ANKET_BASLIK, GRUP, AKTIF FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      'SELECT ANKET_ID, ANKET_BASLIK, GRUP, AKTIF FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLRecCount.Strings = (
      'SELECT Count(*) FROM ('
      'SELECT DA_ANKET.ROWID, DA_ANKET.* FROM DA_ANKET'
      ''
      ')')
    Session = OraSession1
    SQL.Strings = (
      'SELECT'
      
        '(SELECT H.ADI FROM KT_HASTANELER H WHERE H.HASTANE_ID=1) HASTANE' +
        ','
      '(SELECT COUNT(T.YATAK_NO) YATAK  from SU_HBYS_YATAKLARS T) YATAK'
      'FROM DUAL')
    MasterSource = DSANKET
    MasterFields = 'ANKET_ID'
