  object qtoplam: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO DA_ANKET'
      '  (ANKET_ID, ANKET_BASLIK, GRUP, AKTIF)'
      'VALUES'
      '  (:ANKET_ID, :ANKET_BASLIK, :GRUP, :AKTIF)')
    SQLDelete.Strings = (
      'DELETE FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLUpdate.Strings = (
      'UPDATE DA_ANKET'
      'SET'
      
        '  ANKET_ID = :ANKET_ID, ANKET_BASLIK = :ANKET_BASLIK, GRUP = :GR' +
        'UP, AKTIF = :AKTIF'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLLock.Strings = (
      'SELECT ANKET_ID, ANKET_BASLIK, GRUP, AKTIF FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      'SELECT ANKET_ID, ANKET_BASLIK, GRUP, AKTIF FROM DA_ANKET'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLRecCount.Strings = (
      'SELECT Count(*) FROM ('
      'SELECT DA_ANKET.ROWID, DA_ANKET.* FROM DA_ANKET'
      ''
      ')')
    Session = OraSession1
    SQL.Strings = (
      'select count(distinct a.anket_deg_ID) Toplam_denek,'
      '       SUM(A.TOPLAM) toplam_puan,'
      '       (select count(*)'
      '          from da_anket_sorular s'
      '         where s.anket_id = a.anket_id) toplam_soru,'
      
        '      round( ((SUM(A.TOPLAM) / count(distinct a.anket_deg_ID)) *' +
        ' 100) /'
      '             (select count(*)*4'
      '                from da_anket_sorular s'
      '               where s.anket_id = a.anket_id),2) degerlendirme'
      
        '  from (select t.anket_deg_id,t.anket_id,t.cevap,t.anket_soru_id' +
        ','
      '               nvl((select c.sik_puan'
      '                     from da_anket_cvp_anahtar c'
      '                    where c.anket_id = t.anket_id'
      '                      and c.siklar = t.cevap'
      '                      and c.anket_soru_id = t.anket_soru_id),'
      '                   0) TOPLAM'
      '        '
      '          from da_anket_degerlerdirme t, da_anket_deg_bil b'
      '         where '
      
        '          b.anket_deg_id=t.anket_deg_id and b.anket_id = t.anket' +
        '_id /*and t.anket_id=6 --*/ %s'
      
        '          group by t.anket_deg_id,t.anket_id,t.cevap,t.anket_sor' +
        'u_id) a'
      ' group by a.anket_id')
    Left = 632
    Top = 473
  end
